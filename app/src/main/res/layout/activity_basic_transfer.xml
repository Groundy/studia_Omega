<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
								xmlns:tools="http://schemas.android.com/tools"
								xmlns:app="http://schemas.android.com/apk/res-auto"
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								tools:context=".BasicTransferActivity">

				<FrameLayout
												android:layout_width="0dp"
												android:layout_height="0dp"
												app:layout_constraintTop_toTopOf="parent"
												android:layout_marginTop="@dimen/DIM_basicTransfer_marginY"
												app:layout_constraintEnd_toEndOf="parent"
												app:layout_constraintBottom_toBottomOf="parent"
												android:layout_marginBottom="@dimen/DIM_basicTransfer_marginY"
												app:layout_constraintStart_toStartOf="parent"
												app:layout_constraintVertical_bias="0.5"
												android:layout_marginEnd="@dimen/DIM_basicTransfer_marginX"
												android:layout_marginStart="@dimen/DIM_basicTransfer_marginX">

								<LinearLayout
																android:orientation="vertical"
																android:layout_width="wrap_content"
																android:layout_height="wrap_content"
																android:layout_marginTop="0dp"
																android:id="@+id/linearLayout2">

												<LinearLayout
																				android:orientation="vertical"
																				android:layout_width="match_parent"
																				android:layout_height="wrap_content"
																				android:id="@+id/layout_bankList">

																<TextView
																								android:text="@string/GUI_basicTransfer_chooseAccountTextView"
																								android:layout_width="match_parent"
																								android:layout_height="@dimen/DIM_basicTransfer_textFieldY"
																								android:id="@+id/basicTransfer_accountList_textView" />

																<TextView
																								android:text="SPACESSPACESSPACESSPACESSPACESSPACESSPACESSPACES"
																								android:layout_width="match_parent"
																								android:layout_height="@dimen/DIM_basicTransfer_inputFieldY"
																								android:id="@+id/bankNameSpace" />

												</LinearLayout>

												<Space
																				android:layout_width="match_parent"
																				android:layout_height="@dimen/DIM_basicTransfer_spacer_Y" />

												<LinearLayout
																				android:orientation="vertical"
																				android:layout_width="match_parent"
																				android:layout_height="match_parent"
																				android:id="@+id/layout_reciverName">

																<TextView
																								android:text="@string/GUI_basicTransfer_ReciverNameTextView"
																								android:layout_width="match_parent"
																								android:layout_height="@dimen/DIM_basicTransfer_textFieldY"
																								android:id="@+id/basicTransfer_recvierName_textView" />

																<EditText
																								android:layout_width="match_parent"
																								android:layout_height="wrap_content"
																								android:inputType="textPersonName"
																								android:ems="10"
																								android:id="@+id/basicTransfer_reciverName_EditText"
																								android:textColorHint="@color/black"
																								tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

												</LinearLayout>

												<Space
																				android:layout_width="match_parent"
																				android:layout_height="@dimen/DIM_basicTransfer_spacer_Y" />

												<LinearLayout
																				android:orientation="vertical"
																				android:layout_width="match_parent"
																				android:layout_height="match_parent"
																				android:id="@+id/layout_ReciverNumber">

																<TextView
																								android:text="@string/GUI_basicTransfer_ReciverNumberTextView"
																								android:layout_width="match_parent"
																								android:layout_height="@dimen/DIM_basicTransfer_textFieldY"
																								android:id="@+id/basicTransfer_reciverNumber_textView" />

																<EditText
																								android:layout_width="match_parent"
																								android:layout_height="wrap_content"
																								android:inputType="number"
																								android:ems="10"
																								android:id="@+id/basicTransfer_receiverNumber_EditText"
																								android:backgroundTint="@color/black"
																								android:maxLength="26"
																								android:textColorHint="@color/black"
																								tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

												</LinearLayout>

												<Space
																				android:layout_width="match_parent"
																				android:layout_height="@dimen/DIM_basicTransfer_spacer_Y" />

												<LinearLayout
																				android:orientation="vertical"
																				android:layout_width="match_parent"
																				android:layout_height="match_parent"
																				android:id="@+id/layout_Amount">

																<TextView
																								android:text="@string/GUI_basicTransfer_AmountOfTransferTextView"
																								android:layout_width="match_parent"
																								android:layout_height="@dimen/DIM_basicTransfer_textFieldY"
																								android:id="@+id/KwotaTextView" />

																<EditText
																								android:layout_width="match_parent"
																								android:layout_height="wrap_content"
																								android:inputType="numberDecimal"
																								android:ems="10"
																								android:id="@+id/basicTransfer_amount_editText"
																								android:textColorHint="@color/black"
																								tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

												</LinearLayout>

												<Space
																				android:layout_width="match_parent"
																				android:layout_height="@dimen/DIM_basicTransfer_spacer_Y" />

												<LinearLayout
																				android:orientation="vertical"
																				android:layout_width="match_parent"
																				android:layout_height="match_parent"
																				android:id="@+id/layout_Title">

																<TextView
																								android:text="@string/GUI_basicTransfer_TitleOfTransferTextView"
																								android:layout_width="match_parent"
																								android:layout_height="@dimen/DIM_basicTransfer_textFieldY"
																								android:id="@+id/basicTransfer_transfTitle_textView" />

																<EditText
																								android:layout_width="match_parent"
																								android:layout_height="wrap_content"
																								android:inputType="textPersonName"
																								android:ems="10"
																								android:id="@+id/basicTransfer_transferTitle_EditText"
																								android:textColorHint="@color/black"
																								tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

												</LinearLayout>

												<Space
																				android:layout_width="match_parent"
																				android:layout_height="@dimen/DIM_basicTransfer_spacer_Y" />

												<Button
																				android:text="@string/GUI_basicTransfer_goNextButtonText"
																				android:layout_width="match_parent"
																				android:layout_height="wrap_content"
																				android:id="@+id/basicTransfer_goNext_button" />

								</LinearLayout>

				</FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>